{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ES5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "commonjs",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
